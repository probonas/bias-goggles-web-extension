{
  "name": "bias-goggles-browser-ext",
  "version": "0.9.0",
  "description": "A browser extension for the bias-goggles search engine",
  "scripts": {
    "dev": "npx webpack --config ./webpack.dev.js --mode=\"development\"",
    "build": "npx webpack --config ./webpack.prod.js --mode=\"production\"",
    "sign": "npx web-ext sign --api-key=$WEB_EXT_API_KEY --api-secret=$WEB_EXT_API_SECRET"
  },
  "keywords": [
    "bias-goggles",
    "browser",
    "extension",
    "chrome",
    "firefox"
  ],
  "author": "Promponas Giannis",
  "license": "ISC",
  "dependencies": {
    "@fortawesome/fontawesome-free": "^5.12.1",
    "bootstrap": "^4.4.1",
    "chart.js": "^2.7.0",
    "chartjs-plugin-annotation": "^0.5.7",
    "chartjs-plugin-datalabels": "^0.7.0",
    "chartjs-plugin-watermark": "^1.0.3",
    "chartjs-plugin-zoom": "^0.7.5",
    "jquery": "^3.4.1",
    "moment": "^2.24.0",
    "node": "^13.5.0",
    "popper.js": "^1.16.0",
    "querystringify": "^2.1.1"
  },
  "devDependencies": {
    "@types/bootstrap": "^4.3.1",
    "@types/chart.js": "^2.7.0",
    "@types/chrome": "0.0.91",
    "@types/expect.js": "^0.3.29",
    "@types/firefox-webext-browser": "^70.0.1",
    "@types/jasmine": "^3.5.0",
    "@types/moment": "^2.13.0",
    "@types/node": "^13.1.6",
    "@types/popper.js": "^1.11.0",
    "@types/querystringify": "^2.0.0",
    "chartjs-plugin-draggable": "^0.1.6",
    "css-loader": "^3.4.0",
    "expect.js": "^0.3.1",
    "filemanager-webpack-plugin": "^2.0.5",
    "style-loader": "^1.1.2",
    "ts-loader": "^6.2.1",
    "typescript": "^3.7.4",
    "web-ext": "^4.1.0",
    "web-ext-types": "^3.2.1",
    "webpack": "^4.41.4",
    "webpack-cli": "^3.3.10",
    "webpack-merge": "^4.2.2"
  }
